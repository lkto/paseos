<?xml version="1.0" encoding="UTF-8"?>	<widget xmlns = "http://www.w3.org/ns/widgets"		xmlns:gap = "http://phonegap.com/ns/1.0"		id        = "www.paseos.sigtics.org"		versionCode= "1.0"		version   = "1.0">		<name>Paseos</name>		<description>Paseos	</description>		<author href="http://sigtics.org/" email="ingeniero.ember.bejarano@gmail.com">Jesson Ember Bejarano MOsquera		</author>		<preference name="phonegap-version" value="cli-6.5.0" />		<preference name="orientation" value="default" />		<preference name="fullscreen" value="false" />		<preference name="target-device" value="universal" />		<preference name="exit-on-suspend" value="false" />		<preference name="KeyboardDisplayRequiresUserAction" value="false" />		<preference name="HideKeyboardFormAccessoryBar" value="false" />		<preference name="android-installLocation" value="internalOnly" />		<preference name="SplashScreenDelay" value="1000" />		<preference name="ErrorUrl" value=""/>					<preference name="disable-cursor" value="false" />	<platform name="android">        <config-file target="res/xml/config.xml" parent="/*">            <feature name="NetworkStatus">                <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager"/>            </feature>        </config-file>        <config-file target="AndroidManifest.xml" parent="/*">            <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />        </config-file>        <source-file src="src/android/NetworkManager.java" target-dir="src/org/apache/cordova/networkinformation" />    </platform>		<icon src="img/logo.png" gap:platform="android" gap:density="ldpi" />		<icon src="img/logo.png" gap:platform="android" gap:density="mdpi" />		<icon src="img/logo.png" gap:platform="android" gap:density="hdpi" />		<icon src="img/logo.png" gap:platform="android" gap:density="xhdpi" />		<icon src="img/logo.png" gap:platform="android" gap:density="xxhdpi" />		<icon src="img/logo.png" gap:platform="android" gap:density="xxxhdpi" />		<icon src="img/logo.png" />					<access origin="*" />					</widget>